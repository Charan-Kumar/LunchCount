<h1>Listing food_counts</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Date</th>
      <th>Selection</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @food_counts.each do |food_count| %>
      <tr>
        <td><%= food_count.user_id %></td>
        <td><%= food_count.date %></td>
        <td><%= food_count.selection %></td>
        <td><%= link_to 'Show', food_count %></td>
        <td><%= link_to 'Edit', edit_food_count_path(food_count) %></td>
        <td><%= link_to 'Destroy', food_count, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Food count', new_food_count_path %>
